{
  "name": "zachwatkins.github.io",
  "version": "1.2.0",
  "description": "Portfolio for Zachary Watkins",
  "private": true,
  "type": "module",
  "engines": {
    "node": "18.17.1"
  },
  "scripts": {
    "serve": "vitepress preview",
    "dev": "vitepress dev",
    "build": "vitepress build",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview"
  },
  "author": "Zachary Kendall Watkins <zach@zachwatkins.dev>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.4.1",
    "vitepress": "^1.0.0-rc.41",
    "vue": "^3.4.5"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "arrowParens": "always",
    "jsxSingleQuote": true,
    "endOfLine": "lf",
    "quoteProps": "preserve",
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "singleAttributePerLine": true,
    "proseWrap": "never",
    "htmlWhitespaceSensitivity": "strict"
  },
  "dependencies": {
    "reveal.js": "^5.0.4"
  }
}
